          "_name": "contacts3",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"contacts3 marketm4_contacts3\" group=\"Contacts\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"formstyler, datepicker\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n\t\t<header>Show/Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n\t\t<header>Color</header>\n\t\t<input type=\"color\" title=\"Border\" value=\"#ebeaed\" name=\"borderColor\">\n\t\t<input type=\"color\" title=\"Text\" value=\"#919dab\" name=\"colorForm\">\n\t\t<input type=\"color\" title=\"Form\" value=\"#ffffff\" name=\"formBgColor\" condition=\"!transparent\">\n\t\t<input type=\"checkbox\" title=\"Transparent Form\" name=\"transparent\">\n\t\t<header>Bottom</header>\n\t\t<input type=\"checkbox\" title=\"Bottom Title\" name=\"showBottomTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Bottom Text\" name=\"showBottomText\" checked>\n\t\t<select name=\"countItem\" title=\"Count Item\" condition=\"showBottomTitle || showBottomText\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\" selected>2</option>\n\t\t\t<option value=\"3\">3</option>\n\t\t</select>\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/bg1.jpg\" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#182128\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t<input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n\t<div class=\"container\">\n\t\t<div class=\"row justify-content-center text-center\">\n\t\t\t<div class=\"col-xl-6 col-lg-8 col-md-10\">\n\t\t\t\t<p class=\"mbr-text mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\"><br>U kan ons ook contacteren via onderstaande:</p>\n\t\t\t\t<h2 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n\t\t\t\t\tContactformulier</h2>\n\t\t\t\t<div class=\"subscribe__form text-left\">\n\t\t\t\t\t<div mbr-form class=\"d-flex\">\n<!--Formbuilder Form-->\n<form action=\"contact@medispring.be\" name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form form-with-styler\" data-verified=\"contact@medispring.be\">\n<div class=\"row\">\n<div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Bedankt voor je bericht. We komen zo snel mogelijk bij u terug.</div>\n<div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">\n</div>\n</div>\n<div class=\"dragArea row\">\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\" data-for=\"Message\">\n<textarea name=\"Message\" placeholder=\"Uw bericht\" data-form-field=\"Message\" mbr-theme-style=\"display-7\" class=\"form-control\"></textarea>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\" data-for=\"phone\">\n<input type=\"text\" name=\"phone\" placeholder=\"Telefoonnummer\" data-form-field=\"phone\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n</div>\n<div class=\"col-lg-8 col-md-12 col-sm-12 form-group mb-3\" data-for=\"email\">\n<input type=\"email\" name=\"email\" placeholder=\"Email\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n</div>\n<div mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col-lg-4 col-md-12 col-sm-12 text-center input-group-btn\"><a type=\"submit\" class=\"btn btn-lg btn-info\" data-app-placeholder=\"Type Text\">Verzenden</a></div>\n</div>\n</form><!--Formbuilder Form-->\n</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row justify-content-center\" mbr-if=\"showBottomTitle || showBottomText\">\n\t\t\t<div class=\"col-xl-1\" mbr-if=\"countItem >= 3\"></div>\n\t\t\t<div class=\"col-xl-3 col-md-4 col-sm-8 bottom__block\" mbr-if=\"countItem >= 1\">\n\t\t\t\t<p class=\"bottom__title mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".bottom__title\" mbr-if=\"showBottomTitle\">Maatschappelijke - <br>Juridische zetel<br></p>\n\t\t\t\t<p class=\"bottom__text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".bottom__text\" mbr-if=\"showBottomText\">Rue du Stierlinsart 41<br>B-5070 Fosses-la-Ville</p>\n\t\t\t</div>\n\t\t\t<div class=\"col-xl-3 col-md-4 col-sm-8 bottom__block\" mbr-if=\"countItem >= 2\">\n\t\t\t\t<p class=\"bottom__title mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".bottom__title\" mbr-if=\"showBottomTitle\">\n\t\t\t\t\tWerkelijke zetel<br>(postadres)</p>\n\t\t\t\t<p class=\"bottom__text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".bottom__text\" mbr-if=\"showBottomText\">Place de l'Université, 1